<html>
   <head>
      <meta charset="utf8">
      <title>REPORTE PDF</title>
      <style type="text/css">
         html, body {
            margin: 0;
            padding: 0;
            -webkit-print-color-adjust: exact;
            box-sizing: border-box;
         }

         td{
            padding: 0px;
         }

         table{
            padding: 0px;
            text-align: center;
         }

         .esp{
            color: #FFFF00;
         }

         .tablePDF{
           width: 1075px;
         }

         .tam1{font-size: 5px;}
         .tam2{font-size: 4px;}
         .tam3{font-size: 3px;}

         .rowTable{
             width:100%;
             clear:both;
             background: #FFFFFF;
             text-align: center;
         }

         .anchoRow{
            line-height: .5rem;
         }

         .anchoRow2{
            line-height: 4rem;
         }


         .columnRow{ 
           float:left;
           display:table-column;
           text-align: center;
         }

         .borde{border: 1px solid #000000;}

         .borde-ab{border-bottom: 1px solid #000000;}

         .borde-dr{border-right: 1px solid #000000;}

         .rotar{
            -webkit-transform: rotate(-90deg);
            -moz-transform: rotate(-90deg);
            filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
         }

         .box_rotate{
            -moz-transform: rotate(-90deg);  /* FF3.5+ */
            -o-transform: rotate(-90deg);  /* Opera 10.5 */
            -webkit-transform: rotate(-90deg);  /* Saf3.1+, Chrome */
            filter:  progid:DXImageTransform.Microsoft.BasicImage(rotation=3);  /* IE6,IE7 */
            -ms-filter: "progid:DXImageTransform.Microsoft.BasicImage(rotation=3)";
            display: block;
         }

         .rotation  { 
            height: 14px;          
            background: gray; /* need a color for filter */
            -moz-transform: rotate(-90deg);
            -webkit-transform: rotate(-90deg);
            -o-transform: rotate(-90deg);
            -ms-transform: rotate(-90deg);
            transform: rotate(-90deg); 
            float: left; 
            position: relative;
         }

         .amarillo{background-color: #FFFF00;}

         .amarilly{background-color: #FACC2E;}

         .naranja{background-color: #FF8000;}

         .azul{background-color: #013ADF;}
         
         .rojo{background-color: #FF0000;}

         .gris{background-color: #A4A4A4;}

         .crema{background-color: #FBEFF5;}

         .f-1x{width: 64px;}

         .f-2x{width: 105px;}
         
         .f-3x{width: 405px;}
         
         .f-5x{width: 468px;}

         .f-4x{width: 90px;}
         
         .f-6x{width: 173px;}
         
         .f-7x{width: 75px;}

         .f-8x{width: 30px;}

         .f-9x{width: 15px;}

         .f-mx{width: 8px;}

         .f-sx{width: 98px;}

         .f-ix{width: 72px;}

      </style>
