           <li  semana="{{semana}}" mes="{{mes}}" ano="{{ano}}" siguiente="{{siguiente}}" {{#if hecho}}completada="true"{{/if}}>
            <div class="panel">
              <figure class="overlay overlay-hover animation-hover" style="height: 199px;">
                <img class="caption-figure overlay-figure" src="{{#if componentImg}}/uploads/{{componentImg}}{{else}}/photos/sin-imagen{{/if}}.jpg">
                <figcaption class="overlay-panel overlay-background overlay-fade text-xs-center vertical-align">
                    {{#if admin}}
                    <div class="btn-group">
                      <button type="button" class="btn btn-icon btn-pure btn-default" title="Editar" setid="{{id}}" data-tag=project-edit><i class="icon md-edit" aria-hidden="true"></i></button>
                    <button type="button" class="btn btn-icon btn-pure btn-default" title="Eliminar"
                    data-tag="project-delete" setid="{{id}}"><i class="icon md-delete" aria-hidden="true"></i></button>
                  </div>
                  {{/if}}
                  <button type="button" class="btn btn-inverse project-button" onclick="window.open('/componentes?id={{id}}', '_self')">Ver Componentes</button>
                </figcaption>
              </figure>
              <div class="time pull-xs-right">{{siguiente}}
              {{#if hecho}}
                <i class="icon md-check" aria-hidden="true" style="font-size: 22px;font-weight: 900;color: #4caf50;"></i>
              {{/if}}
              </div>
              <div style="display: none" class="hecho">{{hecho}}</div>
              <div style="display: none" class="semana">{{siguiente}}</div>
              <div class="text-truncate nombre">{{componentName}}</div>
            </div>
          </li>